// .babelrc
{
	"presets": ["next/babel"],
	"plugins": [
		["styled-components", { "ssr": true }],
		[
			"module-resolver",
			{
				"root": ["./"],
				"alias": {
					"@components": "./components",
					"@interfaces": "./interfaces",
					"@apis": "apis"
				},
				"extensions": [".js", ".jsx", ".tsx"]
			}
		]
	]
}
